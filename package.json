{
  "name": "react-lottie",
  "version": "2.0.0-alpha.0",
  "description": "lottie animation view for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenqingspring/react-lottie"
  },
  "license": "MIT",
  "scripts": {
    "release": "git add package.json && git commit -m 'chore: bump version number' && standard-version --first-release && git push --follow-tags origin master && npm publish",
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "start": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "postpublish": "npm run publish-storybook",
    "build": "node --experimental-json-modules ./esbuild.config.mjs"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "git-url-parse": "^6.0.1",
    "jsdom": "^9.5.0",
    "lottie-web": "^5.8.1",
    "mocha": "^5.2.0",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^17.0.2",
    "sinon": "^1.17.6",
    "standard-version": "^4.4.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {},
  "main": "dist/index.js",
  "module": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
